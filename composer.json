{
	"name" : "juspay/expresscheckout-php-sdk",
	"description" : "The Juspay ExpressCheckout PHP SDK makes it easier for merchants to integrate the express-checkout APIs in their product",
	"minimum-stability" : "stable",
	"authors" : [{
		"name" : "Juspay Support",
		"email" : "support@juspay.in"
	}],
	"require" : {
		"php": ">=5.3.3",
		"ext-curl" : "*",
		"ext-json" : "*",
		"spomky-labs/jose-bundle": "^2.3"
	},
	"provide" : {
		"ext-sodium": "*"
	},
	"suggest": {
		"ext-gmp": "gmp extension is recommended to improve the sdk performance",
		"ext-bcmath": "BCMath is recommended to improve the sdk performance"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.* || ^9.0"
	},
	"autoload" : {
		"psr-4" : {
			"Juspay\\" : "lib",
			"Juspay\\Model\\" : "lib/Model",
			"Juspay\\Exception\\" : "lib/Exception"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Juspay\\Test\\" : "tests/"
		}
	},
	"license" : "AGPL-3.0-only",
	"support" : {
		"email" : "support@juspay.in"
	},
	"homepage" : "https://www.juspay.in",
	"keywords" : [
		"juspay",
		"expresscheckout",
		"payment processing",
		"api",
		"payment gateway"
	]
}
